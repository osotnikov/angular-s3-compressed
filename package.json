{
  "name": "angular-s3-compressed",
  "version": "0.0.0",
  "config": {
    "templateFile": "template.yml",
    "packagedTemplateFile": "packaged.yaml",
    "region": "us-east-1",
    "stackName": "compressed-website-stack",
    "stackBucketName": "compressed-website-stack-bucket",
    "frontEndBucketName": "compressed-website-front-end-files-bucket"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",

    "setenv": "setx AWS_PROFILE \"myprofile\"",
    "whoami": "aws sts get-caller-identity",
    "create-stack": "aws cloudformation create-stack --stack-name %npm_package_config_stackName% --template-body %npm_package_config_templateFile% --parameters ParameterKey=FrontEndBucketName,ParameterValue=%npm_package_config_frontEndBucketName%",
    "deploy": "aws cloudformation deploy --template %npm_package_config_templateFile% --stack-name %npm_package_config_stackName% --parameters  ParameterKey=FrontEndBucketName,ParameterValue=%npm_package_config_frontEndBucketName%",
    "describe": "aws cloudformation describe-stacks --region %npm_package_config_region% --stack-name %npm_package_config_stackName% >> temp/latest_stack_description.json",
    "delete-last-desc": "del .\\temp\\latest_stack_description.json",
    "clean-deploy": "run-s setenv build package deploy delete-last-desc describe",
    "upload-fe": "run-s delete-files build_and_deploy.bat",
    "delete-stack": "aws cloudformation delete-stack --stack-name %npm_package_config_stackName%",
    "delete-files": "aws s3 rm s3://%npm_package_config_feBucketName% --recursive",
    "delete-all": "run-s delete-files delete-stack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.1.4",
    "@angular/common": "10.1.4",
    "@angular/compiler": "10.1.4",
    "@angular/core": "10.1.4",
    "@angular/forms": "10.1.4",
    "@angular/platform-browser": "10.1.4",
    "@angular/platform-browser-dynamic": "10.1.4",
    "@angular/router": "10.1.4",
    "rxjs": "6.6.0",
    "tslib": "2.0.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "10.0.1",
    "@angular-devkit/build-angular": "0.1001.4",
    "@angular/cli": "10.1.4",
    "@angular/compiler-cli": "10.1.4",
    "@ngtools/webpack": "10.1.4",
    "@types/jasmine": "3.5.0",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "12.11.1",
    "codelyzer": "6.0.0",
    "compression-webpack-plugin": "6.0.2",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.0",
    "karma": "5.0.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.2",
    "karma-jasmine": "4.0.0",
    "karma-jasmine-html-reporter": "1.5.0",
    "protractor": "7.0.0",
    "rename-webpack-plugin": "2.0.0",
    "ts-node": "8.3.0",
    "tslint": "6.1.0",
    "typescript": "4.0.2",
    "npm-run-all": "4.1.5"
  }
}
